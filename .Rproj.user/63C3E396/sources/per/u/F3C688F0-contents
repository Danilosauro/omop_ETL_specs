# reading data 
dicionario_sinasc <- read.csv('dicionario_sinasc_v5_transposto.csv', header=TRUE, sep = ';')


# rewriting data with comma separator
write.table(dicionario_sinasc,file="dicionario_sinasc.csv",sep=",") 



# just testing if it has worked
teste <- read.csv('dicionario_sinasc.csv', header = TRUE, sep = ',')
